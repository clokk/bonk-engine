{
  "name": "bonkjs",
  "version": "0.2.0",
  "description": "2D game engine runtime library â€” importable modules, no forced architecture",
  "type": "module",
  "main": "./dist/bonkjs.js",
  "module": "./dist/bonkjs.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/bonkjs.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.mp3",
    "!dist/**/*.png",
    "!dist/**/*.wav",
    "!dist/**/*.ogg"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "howler": "^2.2.4",
    "matter-js": "^0.19.0",
    "pixi.js": "^8.6.6"
  },
  "devDependencies": {
    "@types/howler": "^2.2.12",
    "@types/matter-js": "^0.19.7",
    "@types/node": "^22.10.7",
    "pixi-filters": "^6.1.5",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.4"
  },
  "keywords": [
    "game-engine",
    "2d",
    "typescript",
    "pixi",
    "matter-js",
    "runtime-library"
  ],
  "author": "Connor Leisz",
  "license": "MIT"
}
